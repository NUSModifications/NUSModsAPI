{
  "name": "nusmods-api",
  "description": "NUSMods API",
  "version": "0.0.1",
  "homepage": "http://api.nusmods.com",
  "author": {
    "name": "NUSModifications",
    "email": "mods@nusmods.com"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server --exec babel-node --watch server | bunyan -o short --color",
    "start": "cross-env NODE_ENV=production gulp | bunyan -o short",
    "start:dev": "cross-env NODE_ENV=development gulp | bunyan -l debug -o short --color",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint gulpfile.babel.js gulp-tasks server",
    "lint:fix": "eslint gulpfile.babel.js gulp-tasks server --fix"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/nusmodifications/nusmods-api.git"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/nusmodifications/nusmods-api/blob/master/LICENSE-MIT"
    }
  ],
  "private": true,
  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "bluebird": "^3.4.7",
    "boom": "^5.1.0",
    "bunyan": "^1.8.5",
    "cheerio": "^0.22.0",
    "chevrotain": "^0.32.1",
    "cross-env": "^5.0.1",
    "diff": "^3.3.0",
    "fs-extra": "^4.0.0",
    "got": "^6.6.3",
    "graphql": "^0.9.4",
    "graphql-server-koa": "^0.7.2",
    "graphql-tools": "^0.11.0",
    "gulp": "gulpjs/gulp.git#4.0",
    "gulp-cli": "^1.2.2",
    "is-binary-path": "^2.0.0",
    "koa": "^2.2.0",
    "koa-bodyparser": "^4.2.0",
    "koa-bunyan-logger": "^2.0.0",
    "koa-compose": "^4.0.0",
    "koa-router": "^7.1.1",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "nusmoderator": "^2.0.0",
    "parse5": "^3.0.1",
    "pdfjs-dist": "^1.6.297",
    "ramda": "^0.24.1",
    "romanify": "^1.0.0",
    "underscore.string": "^3.3.4"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^20.0.3",
    "babel-plugin-transform-object-rest-spread": "^6.19.0",
    "babel-preset-bluebird": "^1.0.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-import-resolver-node": "^0.3.0",
    "eslint-plugin-import": "^2.2.0",
    "jest": "^20.0.4",
    "mock-fs": "^4.2.0",
    "nodemon": "^1.11.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      ".eslintrc.js"
    ],
    "roots": [
      "<rootDir>/__tests__",
      "<rootDir>/__mocks__"
    ]
  }
}
